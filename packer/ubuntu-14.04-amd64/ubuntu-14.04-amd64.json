{
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<enter><esc><enter><wait>",
        "install ",
        "auto ",
        "url=http://{{.HTTPIP}}:{{.HTTPPort}}/preseed.cfg ",
        "locale=en_US.UTF-8 ",
        "country=DE ",
        "language=en ",
        "keymap=de ",
        "hostname={{.Name}} ",
        "domain= ",
        "debconf/frontend=noninteractive ",
        "<enter><wait>",
        "<wait5><enter>"
      ],
      "boot_wait": "10s",
      "disk_size": 51200,
      "guest_os_type": "Ubuntu_64",
      "vm_name": "ubuntu-14.04-amd64",
      "http_directory": ".",
      "iso_url": "http://releases.ubuntu.com/14.04/ubuntu-14.04-server-amd64.iso",
      "iso_checksum_type": "sha512",
      "iso_checksum": "3d547cf075239af2fbb0dabe55382da463e7e2066ba8b81d4d7e50666e30eb2d2d6d971589bafce4946353ad4cbd468b67402f49cb52769e15b383c2a5f21b0f",
      "ssh_username": "sam",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_wait_timeout": "86400s",
      "shutdown_command": "sudo shutdown -hP now",
      "guest_additions_mode": "disable",
      "vboxmanage": [
        [ "modifyvm", "{{.Name}}", "--memory", "512" ],
        [ "modifyvm", "{{.Name}}", "--cpus", "2" ]
      ]
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "compression_level": "9",
      "output": "ubuntu-14.04-amd64_{{.Provider}}.box"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "execute_command": "echo 'vagrant'|{{.Vars}} sudo -S -E bash '{{.Path}}'",
      "scripts": [
        "../scripts/packages.sh",
        "../scripts/access.sh",
        "../scripts/cleanup.sh"
      ]
    }
  ]
}
